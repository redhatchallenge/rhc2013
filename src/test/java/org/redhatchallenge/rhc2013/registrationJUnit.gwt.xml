<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module>
    <!-- Inherit our applications main module.                      -->
    <inherits name='org.redhatchallenge.rhc2013.registration'/>
    <inherits name="com.google.gwt.user.Debug"/>
    <inherits name="com.google.gwt.event.dom.DomEvent"/>
    <inherits name="org.redhatchallenge.rhc2013.registration"/>
    <!-- Specify the path to any remote services.                   -->
    <servlet path="/register/greet" class="org.redhatchallenge.rhc2013.server.RegisterServiceImpl" />
    <servlet path="/AuthenticationService" class="org.redhatchallenge.rhc2013.server.AuthenticationServiceImpl"/>
    <servlet path="/ProfileService" class="org.redhatchallenge.rhc2013.server.ProfileServiceImpl"/>
</module>

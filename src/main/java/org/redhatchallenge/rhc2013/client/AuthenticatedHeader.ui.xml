<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="en">

    <ui:with field="res" type="org.redhatchallenge.rhc2013.resources.Resources"/>

    <g:HTMLPanel>
        <div id="sm-logo"></div><!-- /#sm-logo -->

        <div id="header-links">

            <!--	<ul>
                    <li class="first"><a href=""></a></li>
                </ul>-->

            <div class="{res.grid.clear}"/>

            <a target="_blank" class="header-button" href="http://www.redhat.com"><img src="images/redhat_logo.png" /></a><!-- /.header-button -->

        </div><!-- /#header-links -->

        <!--<div id="top_level_message"><font size="14pt">BUILDING</font></div>

        <div id="supporting_message"><font size="4pt">THE NEXT CLOUD</font><br>
        <font size="+3">GENERATION</font></div>-->

        <div class="{res.main.navBand}">
            <div class="{res.main.primaryNav}">
                <ul class="{res.main.sfMenu}">
                    <ui:msg description="Navigation panel">
                    <li class="{res.main.navFirst}"><g:Hyperlink ui:field="indexLink" text="Home" styleName="{res.main.link}"/></li>
                    <li class="{res.main.navLink}"><g:Hyperlink ui:field="tcLink" text="Terms &amp; Conditions" styleName="{res.main.link}" targetHistoryToken="tc"/></li>
                    <li class="{res.main.navLink}"><g:Hyperlink ui:field="contestDetailsLink" text="Details" styleName="{res.main.link}" targetHistoryToken="details"/></li>
                    <li class="{res.main.navFirst}" style="margin-left: 400px"><g:Hyperlink ui:field="profileLink" text="edit profile" styleName="{res.main.link}" targetHistoryToken="profile"/></li>
                    <li class="{res.main.navLink}"><g:Hyperlink ui:field="logoutLink" text="logout" styleName="{res.main.link}"/></li>
                    <li class="{res.main.navLink}"><g:Label ui:field="welcomeLabel" styleName="{res.main.gwtLabel}"/></li>
                    </ui:msg>
                </ul><!-- /#nav -->
            </div><!-- /#primary-nav -->
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="res" type="org.ayrx.rhchallenge.resources.Resources"/>

    <g:HTMLPanel ui:field="htmlPanel">
        <div class="{res.grid.clear}"/>
        <div class="{res.grid.container_12} {res.grid.clearfix}">
            <div id="main">
                <div class="{res.grid.grid_12}">
                    <div id="sidebar" style="float: right">
                        <ul class="{res.main.socialIcons}">
                            <li><span style="position: relative; top: 4px; left: -2px;"><strong>Connect:</strong></span></li>
                            <li><a target="_blank" href="http://www.facebook.com/redhatinc?fref=ts"><img src="images/social_media_icons-16.png" alt="Facebook" /></a></li><li><a target="_blank" href="http://www.twitter.com/red_hat_apac"><img src="images/social_media_icons-17.png" alt="Twitter" /></a></li>
                        </ul>
                    </div>

                    <div class="{res.grid.grid_8} {res.grid.alpha}" style="margin-right: 40px">
                        <div id="table">
                            <table ui:field="table" style="width: 463px">
                                <tr>
                                    <td colspan="2">
                                        <h1>EDIT PROFILE</h1>
                                    </td>
                                </tr>
                                <tr><td colspan="2">&nbsp;</td></tr>
                                <tr>
                                    <td colspan="2">
                                        <font color="#CC0000">PERSONAL DETAILS</font><div class="border2" style="width:360px; color: #CC0000; border-bottom: solid 3px"/>
                                    </td>
                                </tr>
                                <tr><td colspan="2">&nbsp;</td></tr>
                                <tr>
                                    <td style="width: 174px" class="{res.main.rowform}">Email Address:</td>
                                    <td style="width: 268px" class="{res.main.rowform}"><g:TextBox ui:field="emailField" maxLength="120" visibleLength="33"/></td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">Current Password:</td>
                                    <td class="{res.main.rowform}"><g:PasswordTextBox ui:field="currentPasswordField" maxLength="120" visibleLength="33"/></td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">Password:</td>
                                    <td class="{res.main.rowform}"><g:PasswordTextBox ui:field="passwordField" maxLength="120" visibleLength="33"/></td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">Confirm Password:</td>
                                    <td class="{res.main.rowform}"><g:PasswordTextBox ui:field="confirmPasswordField" maxLength="120" visibleLength="33"/></td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">First Name:</td>
                                    <td class="{res.main.rowform}"><g:TextBox ui:field="firstNameField" maxLength="120" visibleLength="33"/></td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">Last Name:</td>
                                    <td class="{res.main.rowform}"><g:TextBox ui:field="lastNameField" maxLength="120" visibleLength="33"/></td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">Country:</td>
                                    <td class="{res.main.rowform}">
                                        <g:ListBox ui:field="countryField" visibleItemCount="1">
                                            <g:item value="1">Singapore</g:item>
                                            <g:item value="2">Malaysia</g:item>
                                            <g:item value="3">Thailand</g:item>
                                            <g:item value="4">China</g:item>
                                            <g:item value="5">Hong Kong</g:item>
                                            <g:item value="6">Taiwan</g:item>
                                        </g:ListBox>

                                        <g:ListBox ui:field="regionField" visibleItemCount="1" visible="false">
                                            <g:item value="1">Region 1</g:item>
                                            <g:item value="2">Region 2</g:item>
                                            <g:item value="3">Region 3</g:item>
                                            <g:item value="4">Region 4</g:item>
                                            <g:item value="5">Region 5</g:item>
                                            <g:item value="6">Region 6</g:item>
                                            <g:item value="7">Others</g:item>
                                        </g:ListBox>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">Contact:</td>
                                    <td class="{res.main.rowform}">
                                        <g:ListBox ui:field="countryCodeField" visibleItemCount="1">
                                            <g:item value="1">65</g:item>
                                            <g:item value="2">60</g:item>
                                            <g:item value="3">66</g:item>
                                            <g:item value="4">86</g:item>
                                            <g:item value="5">852</g:item>
                                            <g:item value="6">886</g:item>
                                        </g:ListBox>
                                        <g:TextBox ui:field="contactField" maxLength="30" visibleLength="19"/>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">School:</td>
                                    <td class="{res.main.rowform}"><g:TextBox ui:field="schoolField" maxLength="120" visibleLength="33"/></td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">Lecturer's First Name:</td>
                                    <td class="{res.main.rowform}"><g:TextBox ui:field="lecturerFirstNameField" maxLength="120" visibleLength="33"/></td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">Lecturer's Last Name:</td>
                                    <td class="{res.main.rowform}"><g:TextBox ui:field="lecturerLastNameField" maxLength="120" visibleLength="33"/></td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">Lecturer's Email:</td>
                                    <td class="{res.main.rowform}"><g:TextBox ui:field="lecturerEmailField" maxLength="120" visibleLength="33"/></td>
                                </tr>
                                <tr>
                                    <td class="{res.main.rowform}">Preferred Language:</td>
                                    <td class="{res.main.rowform}">
                                        <g:ListBox ui:field="languageField" visibleItemCount="1">
                                            <g:item value="1">English</g:item>
                                            <g:item value="2">Chinese (Simplified)</g:item>
                                            <g:item value="3">Chinese (Traditional)</g:item>
                                        </g:ListBox>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2"><g:VerticalPanel ui:field="recaptchaPanel"/></td>
                                </tr>
                                <tr>
                                    <td>&nbsp;</td>
                                    <td><g:Image ui:field="updateButton" url="images/saveprofile_button.png"/></td>
                                </tr>
                                <tr>
                                    <td><g:Label ui:field="errorLabel"/></td>
                                </tr>
                            </table>
                        </div>
                        <div class="{res.main.panel02} {res.grid.clearfix}" style="margin-top: 1em;"/>
                    </div>
                    <div class="{res.grid.clear}"/>
                </div>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>

